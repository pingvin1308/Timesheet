{
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9210/",
          "indexFormat": "timesheets-{0:yyyy.MM}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "TimesheetContext": "Server=localhost;Database=TimesheetDB;Trusted_Connection=True;"
  },
  "AllowedHosts": "*"
}
